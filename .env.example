# Copy this to .env and fill in your own values. Do not commit real secrets.

# Required: access to models via OpenRouter
OPENROUTER_API_KEY=

# Optional: observability/logging
WANDB_API_KEY=
OPENPIPE_API_KEY=

# Optional: data generation controls
# Set to 1 to force re-generation of training inputs
FORCE_REGENERATE_TRAINING_INPUTS=

# Optional: provider/infra credentials
# If using RunPod with SkyPilot
RUNPOD_API_KEY=
# If pushing logs/models to S3
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=
BACKUP_BUCKET=
